{
  "name": "me-ui",
  "version": "0.1.0",
  "scripts": {
    "nx": "nx",
    "start": "npm run build-dynamic-env && nx serve --hmr --project=deep-ui --watch",
    "build:dynamic-env": "npm run build-dynamic-env",
    "build:me-material": "npm run build-dynamic-env && nx build me-material",
    "build:me-material:watch": "npm run build-dynamic-env && node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build me-material --watch",
    "build:production": "nx run-many --target=build --projects=deep-ui --verbose --parallel=5 --configuration=production",
    "build:staging": "nx run-many --target=build --projects=deep-ui --parallel=5 --configuration=staging",
    "lint": "nx lint",
    "e2e": "nx e2e",
    "test-sanity": "nx reset && TAILWIND_MODE=build nx test me-material --configuration=production --watch=false --browsers=ChromeHeadlessNoSandbox --reporters=junit,dots && nx test deep-ui-shared-components --configuration=production --watch=false --browsers=ChromeHeadlessNoSandbox --reporters=junit,dots",
    "test-all:ci": "nx reset && TAILWIND_MODE=build nx run-many --target=test --parallel=3  --configuration=production --watch=false --browsers=ChromeHeadlessNoSandbox --reporters=junit,dots",
    "test-all:ci-local": "TAILWIND_MODE=build nx run-many --target=test --parallel=3  --configuration=production --watch=false --browsers=ChromeHeadlessNoSandbox --reporters=junit,dots",
    "affected:ci": "nx clear-cache && nx show projects --affected --base=origin/master && TAILWIND_MODE=build nx affected -t test --base=origin/master --parallel=3 --configuration=production --watch=false --browsers=ChromeHeadlessNoSandbox --reporters=junit,dots",
    "affected:ci-local": "nx show projects --affected --base=origin/master && TAILWIND_MODE=build nx affected -t test --base=origin/master --parallel=3 --configuration=production --watch=false --browsers=ChromeHeadlessNoSandbox --reporters=junit,dots",
    "unit-test:deep-ui": "TAILWIND_MODE=build nx test --project=deep-ui --reporters=spec,kjhtml",
    "unit-test:deep-ui:source-map": "TAILWIND_MODE=build nx test --project=deep-ui --source-map=false --reporters=spec",
    "unit-test:deep-ui:code-coverage": "TAILWIND_MODE=build nx test --project=deep-ui --code-coverage --reporters=dots",
    "unit-test:me-material": "TAILWIND_MODE=build nx test  --project=me-material  --reporters=spec,kjhtml",
    "unit-test:me-material:code-coverage": "TAILWIND_MODE=build nx test --project=me-material --code-coverage --reporters=dots",
    "unit-test:me-materia:w:source-map": "TAILWIND_MODE=build nx test --project=me-material --source-map=false --reporters=spec ",
    "unit-test:deep-ui:ci": "TAILWIND_MODE=build nx run-many --target=test --parallel=5 --watch=false --browsers=ChromeHeadlessNoSandbox --reporters=junit,dots",
    "unit-test:me-material:ci": "TAILWIND_MODE=build nx test --watch=false --browsers=ChromeHeadlessNoSandbox --project=me-material --reporters=junit,dots",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint --fix",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "dep-graph": "nx dep-graph",
    "build-dynamic-env": "ts-node -O '{\"module\": \"commonjs\"}' ./scripts/generate-dynamic-env.ts",
    "postinstall": "node ./decorate-angular-cli.js",
    "explore": "source-map-explorer dist/apps/**/*.js",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "32.3.2",
